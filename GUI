        // B7 is a nested class in the startListener class. This is the class which is the actionListener for the  button
        public class B7  implements ActionListener
        {
            //actionPerformed is the method that is called when the  button is clicked
            public void actionPerformed (ActionEvent event)
            {
                //the user is only able to click on the button if the button is still blank


                if(game.getNum(0,6) == 0)
                {

                    number7 = new JTextField(1);
                    number7.addActionListener(new Number7());

                    grid[0][6].add(number7);

                    frame.setVisible(true);
                }
            }
            //-----------------------------------------------------------------------------------------------------------------------------------------
            public class Number7 implements ActionListener
            {
                public void actionPerformed (ActionEvent event)
                {
                    actualNum = number7.getText();

                    //if the user has entered a blank input then the button is reset or else it checks whether the number entered is the correct number
                    //if it is the correct number, then the number is placed at that button or else a popup window informing the user that the number is 
                    //correct is displayed       
                    if (actualNum.equals(""))
                    {
                        grid[0][6].remove(number1);
                        frame.setVisible(true);
                    }
                    else
                    {

                        numNum = Integer.parseInt(actualNum);

                        if(game.getNumAnswer(0,6) == numNum)
                        {

                            grid[0][6].remove(number7);
                            grid[0][6].setText(actualNum);

                            frame.setVisible(true);

                            game.setNum(0,6, numNum);
                        }
                        else
                        {
                            JFrame frame=new JFrame("Wrong Number Entered");

                            JPanel panel = new JPanel();
                            panel.setPreferredSize (new Dimension (700,200));

                            JLabel label1 = new JLabel("The number entered does not belong at this location.");
                            JLabel label2 = new JLabel("Please enter a different number");

                            panel.add(label1);
                            panel.add(label2);

                            frame.getContentPane().add(panel);
                            frame.setBackground (Color.green);
                            frame.pack();
                            frame.setPreferredSize (new Dimension (700,200));
                            frame.setVisible(true);

                        }
                    }
                }
            }
        }
