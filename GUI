        // B69 is a nested class in the startListener class. This is the class which is the actionListener for the  button
        public class B69  implements ActionListener
        {
            //actionPerformed is the method that is called when the  button is clicked
            public void actionPerformed (ActionEvent event)
            {
                //the user is only able to click on the button if the button is still blank

                if(game.getNum(7,5) == 0)
                {

                    number69 = new JTextField(1);
                    number69.addActionListener(new Number69());

                    grid[7][5].add(number69);

                    frame.setVisible(true);
                }
            }
            //-----------------------------------------------------------------------------------------------------------------------------------------
            public class Number69 implements ActionListener
            {
                public void actionPerformed (ActionEvent event)
                {
                    actualNum = number69.getText();

                    //if the user has entered a blank input then the button is reset or else it checks whether the number entered is the correct number
                    //if it is the correct number, then the number is placed at that button or else a popup window informing the user that the number is 
                    //correct is displayed       
                    if (actualNum.equals(""))
                    {
                        grid[7][5].remove(number69);
                        frame.setVisible(true);

                    }
                    else
                    {

                        numNum = Integer.parseInt(actualNum);


                        if(game.getNumAnswer(7,5) == numNum)
                        {

                            grid[7][5].remove(number69);
                            grid[7][5].setText(actualNum);

                            frame.setVisible(true);

                            game.setNum(7,5, numNum);
                        }
                        else
                        {
