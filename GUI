                    grid[8][8].add(number81);

                    frame.setVisible(true);
                }
            }
            //-----------------------------------------------------------------------------------------------------------------------------------------
            public class Number81 implements ActionListener
            {
                public void actionPerformed (ActionEvent event)
                {
                    actualNum = number81.getText();

                    //if the user has entered a blank input then the button is reset or else it checks whether the number entered is the correct number
                    //if it is the correct number, then the number is placed at that button or else a popup window informing the user that the number is 
                    //correct is displayed       
                    if (actualNum.equals(""))
                    {
                        grid[8][8].remove(number81);
                        frame.setVisible(true);

                    }
                    else
                    {
                        numNum = Integer.parseInt(actualNum);


                        if(game.getNumAnswer(8,8) == numNum)
                        {

                            grid[8][8].remove(number81);
                            grid[8][8].setText(actualNum);

                            frame.setVisible(true);


                            game.setNum(8,8, numNum);
                        }
                        else
                        {
                            JFrame frame=new JFrame("Wrong Number Entered");

                            JPanel panel = new JPanel();
                            panel.setPreferredSize (new Dimension (700,200));

                            JLabel label1 = new JLabel("The number entered does not belong at this location.");
                            JLabel label2 = new JLabel("Please enter a different number");

                            panel.add(label1);
                            panel.add(label2);

                            frame.getContentPane().add(panel);
                            frame.setBackground (Color.green);
                            frame.pack();
                            frame.setPreferredSize (new Dimension (700,200));
                            frame.setVisible(true);

                        }
                    }
                }

            }
        }

//==============================================================================================================================================
    }
    //==============================================================================================================================================
// InstructionsListener is a nested class in the GUI class. This is the class which is the actionListener for the instructions button
    public class InstructionsListener implements ActionListener
    {
        //actionPerformed is the method that is called when the  button is clicked
        public void actionPerformed (ActionEvent event)
        {
            JFrame frame=new JFrame("Instructions");

            JPanel panel = new JPanel();
            panel.setPreferredSize (new Dimension (700,500));

            JLabel label1 = new JLabel("The objective of the game is to fill all the blank squares in a game with the correct numbers.");
            JLabel label2 = new JLabel("There are three very simple constraints to follow:");
            JLabel label3 = new JLabel("Every row of 9 numbers must include all digits 1 through 9 in any order");
            JLabel label4 = new JLabel("Every column of 9 numbers must include all digits 1 through 9 in any order");
            JLabel label5 = new JLabel("Every 3 by 3 subsection of the 9 by 9 square must include all digits 1 through 9");

            panel.add(label1);
            panel.add(label2);
            panel.add(label3);
            panel.add(label4);
            panel.add(label5);

            frame.getContentPane().add(panel);
            frame.setBackground (Color.orange);
            frame.pack();
            frame.setPreferredSize (new Dimension (700,500));
            frame.setVisible(true);


        }
    }
//===============================================================================================================================================
}
