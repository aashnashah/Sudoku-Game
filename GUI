    //This is the constructor GUI, which the PDriver class uses to create the initial button
    public GUI(String run)
    {
        ImageIcon bg= new ImageIcon("bg final.jpg");
        JLabel backgroundImage = new JLabel(bg);

        //A panel is created, the color and size are set
        JPanel panel = new JPanel();
        panel.setPreferredSize (new Dimension (700,200));
        panel.setPreferredSize (new Dimension (700,200));
        panel.setOpaque(false);
        panel.setPreferredSize (new Dimension (700, 500));

        LayoutManager overlay = new OverlayLayout(panel);

        panel.setLayout(overlay);

        JLayeredPane lPanel = new JLayeredPane();
        lPanel.setPreferredSize(new Dimension(700, 500));

        //A panel is created, the color and size are set
        JPanel subPanel1 = new JPanel();
        subPanel1.setMaximumSize(new Dimension (150,150));

        //A panel is created, the color and size are set
        JPanel subPanel2 = new JPanel();
        subPanel2.setPreferredSize (new Dimension (700, 500));
        subPanel2.add(backgroundImage);


        //The various buttons are instantiated
        start = new JButton ("Start");
        instructions = new JButton ("Instructions");

        //the buttons and labels are added to the subPanel1   
        subPanel1.add(start);
        subPanel1.add(instructions);
        subPanel1.setLayout(new GridLayout(2,0,0,5));

        lPanel.add(subPanel1,new Integer(2) );
        lPanel.add(subPanel2, new Integer(1));

        panel.add(subPanel2);
        panel.add(subPanel1);

        add(panel);

        setPreferredSize (new Dimension (700, 500));

        //ActionListeners are added to the various buttons
        start.addActionListener (new StartListener());
        instructions.addActionListener (new InstructionsListener());
    }


    //===========================================================================================================================================
    // StartListener is a nested class in the GUI class. This is the class which is the actionListener for the button  
    public class StartListener implements ActionListener
    {
        //various components are created
        JButton[][] grid= new JButton[11][11];

        Sudoku game = new Sudoku("newwww");

        JFrame frame=new JFrame("New Game");

        JPanel panel = new JPanel();

        //actionPerformed is the method that is called when the start button is clicked
        public void actionPerformed (ActionEvent event)
        {
            panel.setPreferredSize (new Dimension (1500, 700));
            panel.setLayout(new GridLayout(11,11));

            //the sudoku game calls various methods 
            game.selectNums();
            game.nextEmpty();
            game.placeNums(0,0);
            game.createGame();

            //depending on the game, the various buttons are assigned numbers or left blank for the user to fill in
            for(int x= 0; x < 9; x++)
            {
                for(int y = 0; y < 9; y++)
                {
                    if (game.getNum(x,y) == 0)
                    {
                        grid[x][y]=new JButton();

                    }
                    else
                    {

                        numberInt = game.getNum(x,y);

                        grid[x][y] = new JButton("" + numberInt + "");
                    }

                    if(y == 2 || y == 5)
                    {
                        panel.add(grid[x][y]);

                        panel.add(Box.createRigidArea(new Dimension(0, 0)));

                    }
                    //this is to create the actuals layout of the game
                    else if ((x == 3 && y == 0))
                    {
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));

                        panel.add(grid[x][y]);


                    }

                    else if (x == 6 && y ==0)
                    {
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));
                        panel.add(Box.createRigidArea(new Dimension(0, 0)));

                        panel.add(grid[x][y]);
                    }
                    else
                    {
                        panel.add(grid[x][y]);
                    }

                }

            }
            frame.getContentPane().add(panel);
            frame.setBackground (Color.orange);
            frame.pack();
            frame.setPreferredSize (new Dimension (1500, 700));
            frame.setVisible(true);


            //actions listeners are added for the various buttons
            grid[0][0].addActionListener (new B1());
            grid[0][1].addActionListener (new B2());
            grid[0][2].addActionListener (new B3());
            grid[0][3].addActionListener (new B4());
            grid[0][4].addActionListener (new B5());
            grid[0][5].addActionListener (new B6());
            grid[0][6].addActionListener (new B7());
            grid[0][7].addActionListener (new B8());
            grid[0][8].addActionListener (new B9());

            grid[1][0].addActionListener (new B10());
            grid[1][1].addActionListener (new B11());
            grid[1][2].addActionListener (new B12());
            grid[1][3].addActionListener (new B13());
            grid[1][4].addActionListener (new B14());
            grid[1][5].addActionListener (new B15());
            grid[1][6].addActionListener (new B16());
            grid[1][7].addActionListener (new B17());
            grid[1][8].addActionListener (new B18());

            grid[2][0].addActionListener (new B19());
            grid[2][1].addActionListener (new B20());
            grid[2][2].addActionListener (new B21());
            grid[2][3].addActionListener (new B22());
            grid[2][4].addActionListener (new B23());
            grid[2][5].addActionListener (new B24());
            grid[2][6].addActionListener (new B25());
            grid[2][7].addActionListener (new B26());
            grid[2][8].addActionListener (new B27());

            grid[3][0].addActionListener (new B28());
            grid[3][1].addActionListener (new B29());
            grid[3][2].addActionListener (new B30());
            grid[3][3].addActionListener (new B31());
            grid[3][4].addActionListener (new B32());
            grid[3][5].addActionListener (new B33());
            grid[3][6].addActionListener (new B34());
            grid[3][7].addActionListener (new B35());
            grid[3][8].addActionListener (new B36());

            grid[4][0].addActionListener (new B37());
            grid[4][1].addActionListener (new B38());
            grid[4][2].addActionListener (new B39());
            grid[4][3].addActionListener (new B40());
            grid[4][4].addActionListener (new B41());
            grid[4][5].addActionListener (new B42());
            grid[4][6].addActionListener (new B43());
            grid[4][7].addActionListener (new B44());
            grid[4][8].addActionListener (new B45());

            grid[5][0].addActionListener (new B46());
            grid[5][1].addActionListener (new B47());
            grid[5][2].addActionListener (new B48());
            grid[5][3].addActionListener (new B49());
            grid[5][4].addActionListener (new B50());
            grid[5][5].addActionListener (new B51());
            grid[5][6].addActionListener (new B52());
            grid[5][7].addActionListener (new B53());
            grid[5][8].addActionListener (new B54());

            grid[6][0].addActionListener (new B55());
            grid[6][1].addActionListener (new B56());
            grid[6][2].addActionListener (new B57());
            grid[6][3].addActionListener (new B58());
            grid[6][4].addActionListener (new B59());
            grid[6][5].addActionListener (new B60());
            grid[6][6].addActionListener (new B61());
            grid[6][7].addActionListener (new B62());
            grid[6][8].addActionListener (new B63());

            grid[7][0].addActionListener (new B64());
            grid[7][1].addActionListener (new B65());
            grid[7][2].addActionListener (new B66());
            grid[7][3].addActionListener (new B67());
            grid[7][4].addActionListener (new B68());
            grid[7][5].addActionListener (new B69());
            grid[7][6].addActionListener (new B70());
            grid[7][7].addActionListener (new B71());
            grid[7][8].addActionListener (new B72());

            grid[8][0].addActionListener (new B73());
            grid[8][1].addActionListener (new B74());
            grid[8][2].addActionListener (new B75());
            grid[8][3].addActionListener (new B76());
